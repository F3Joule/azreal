{
  "name": "azreal",
  "description": "UI for Airzoom project",
  "version": "0.0.1",
  "author": "Oleh Mell <oleh.melnechyk@gmail.com>",
  "keywords": [
    "next",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm -f public/themes/*.min.css && next build",
    "dev": "next",
    "lint": "tsc && eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start",
    "generate:types": "graphql-codegen --config ./types-codegen.yml",
    "generate:hooks": "graphql-codegen --config ./hooks-codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olehmell/azreal/ui"
  },
  "bugs": {
    "url": "https://github.com/olehmell/azreal/issues"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "28.3.1",
    "@next/bundle-analyzer": "^9.5.3",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.4",
    "apollo-boost": "^0.4.7",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^5.0.0",
    "faker": "^5.1.0",
    "glob": "^7.1.6",
    "graphql": "^15.0.0",
    "graphql.macro": "^1.4.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
