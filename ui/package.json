    "eslint-plugin-react-hooks": "^4.1.0",
    "iniparser": "^1.0.5",
    "null-loader": "^4.0.0",
    "prettier": "^2.1.1",
    "serve": "^11.3.2",
    "typescript": "^4.0.2"
  }
